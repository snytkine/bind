{
  "name": "bind",
  "version": "0.1.1",
  "description": "Dependency Injection Container for Javascript",
  "private": false,
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prebuild": "node ./node-build-os.js clean",
    "build": "tsc --pretty",
    "pretest": "npm run build",
    "test": "nyc mocha build/tests/ --recursive",
    "format": "eslint src --ext .ts --fix",
    "lint": "eslint src --ext .ts",
    "coverage": "mv coverage/coverage-final.json coverage/coverage.json"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "typings": "build/types/index.d.ts",
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/snytkine/bind.git "
  },
  "keywords": [
    "Dependency Injection",
    "Inversion of Control",
    "Decorators",
    "Node.js",
    "Framework",
    "Typescript"
  ],
  "author": {
    "name": "Dmitri Snytkine",
    "email": "dmitri.snytkine@adp.com"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.9",
    "require-from-string": "^1.2.1",
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "typescript": "^3.7.5",
    "@types/debug": "0.0.29",
    "@types/node": "^8.11.0"
  }
}
