{
  "name": "bind-di",
  "version": "1.0.0",
  "description": "Dependency Injection Framework for TypeScript",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prebuild": "node ./node-build-os.js clean",
    "build": "tsc --pretty",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=TEST NODE_ENC=TEST jest",
    "test:watch": "cross-env NODE_ENV=TEST NODE_ENC=TEST jest --watch",
    "test:coverage": "cross-env NODE_ENV=TEST NODE_ENC=TEST jest --coverage",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*.ts",
    "format": "eslint src --ext .ts --fix",
    "lint": "eslint src --ext .ts"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "engines": {
    "node": ">=8.11.0"
  },
  "typings": "build/types/index.d.ts",
  "files": [
    "build/",
    "README.md",
    "LICENSE"
  ],
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/snytkine/bind.git "
  },
  "keywords": [
    "Dependency Injection",
    "Inversion of Control",
    "Decorators",
    "Node.js",
    "Framework",
    "Typescript"
  ],
  "author": {
    "name": "Dmitri Snytkine",
    "email": "dmitri.snytkine@adp.com"
  },
  "bugs": {
    "url": "https://github.com/snytkine/bind/issues"
  },
  "homepage": "https://github.com/snytkine/bind#readme",
  "dependencies": {
    "build": "^0.1.4",
    "debug": "^4.1.1",
    "npm": "^6.14.4",
    "reflect-metadata": "^0.1.13",
    "run": "^1.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  }
}
